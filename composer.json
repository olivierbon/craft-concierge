{
    "name": "olivierbon/craft-concierge",
    "description": "Simple user moderation plugin for Craft",
    "type": "craft-plugin",
    "version": "2.1.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "concierge",
        "moderation",
        "users"
    ],
    "support": {
        "docs": "http://concierge.olivierbon.com/documentation/",
        "issues": "http://concierge.olivierbon.com"
    },
    "license": "proprietary",
    "authors": [
        {
            "name": "Olivier Bon",
            "homepage": "http://olivierbon.com",
            "email": "help@olivierbon.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "craftcms/cms": "^3.0.0-RC1"
    },
    "autoload": {
        "psr-4": {
          "olivierbon\\concierge\\": "src/"
        }
    },
    "extra": {
        "name": "Concierge",
        "handle": "concierge",
        "class": "olivierbon\\concierge\\Concierge"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "craftcms/rector": "dev-main"
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
